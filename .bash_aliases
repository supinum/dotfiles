alias lnn="cd ~/Dropbox/notes;clear;ls -Ar1t"
alias nnn="cd ~/Dropbox/notes;vim"
alias nng="cd ~/nnginx;vim"
alias bu="vim ~/.bash_user"
alias bur="source ~/.bash_user"
alias vimc="vim ~/.vimrc"
alias bp="vim ~/.zshrc"
alias ba="vim ~/.bash_aliases"
# alias bag="cat ~/.bash_aliases | grep "
alias bar="source ~/.bash_aliases"
alias bpr="source ~/.zshrc";
alias c="clear"
